{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/services/api';\nexport default {\n  data() {\n    return {\n      wastes: [],\n      newWaste: {\n        name: '',\n        description: '',\n        quantity: 0\n      },\n      showAddWasteForm: false,\n      showDeleteModal: false,\n      deleteWasteId: null\n    };\n  },\n  created() {\n    this.fetchWastes();\n  },\n  methods: {\n    fetchWastes() {\n      api.getAll().then(response => {\n        this.wastes = response.data;\n      }).catch(error => {\n        console.error('Error fetching wastes:', error);\n      });\n    },\n    createWaste() {\n      api.createWaste(this.newWaste).then(response => {\n        this.wastes.push(response.data);\n        this.newWaste = {\n          name: '',\n          description: '',\n          quantity: 0\n        };\n        this.showAddWasteForm = false;\n      }).catch(error => {\n        console.error('Error creating waste:', error);\n      });\n    },\n    confirmDeleteWaste(waste) {\n      this.showDeleteModal = true;\n      this.deleteWasteId = waste._id;\n    },\n    deleteWaste() {\n      api.delete(this.deleteWasteId).then(() => {\n        this.wastes = this.wastes.filter(waste => waste._id !== this.deleteWasteId);\n        this.closeDeleteModal();\n      }).catch(error => {\n        console.error('Error deleting waste:', error);\n      });\n    },\n    closeDeleteModal() {\n      this.showDeleteModal = false;\n      this.deleteWasteId = null;\n    },\n    toggleAddWasteForm() {\n      this.showAddWasteForm = !this.showAddWasteForm;\n    },\n    editWaste(waste) {\n      // Implementation for editing waste\n    }\n  }\n};","map":{"version":3,"names":["api","data","wastes","newWaste","name","description","quantity","showAddWasteForm","showDeleteModal","deleteWasteId","created","fetchWastes","methods","getAll","then","response","catch","error","console","createWaste","push","confirmDeleteWaste","waste","_id","deleteWaste","delete","filter","closeDeleteModal","toggleAddWasteForm","editWaste"],"sources":["/home/edward/summer-project/frontend/src/components/forms/WasteForm.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Waste Management</h1>\n    <table class=\"data-table\">\n      <thead>\n        <tr>\n          <th>Name</th>\n          <th>Description</th>\n          <th>Quantity</th>\n          <th>Actions</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"waste in wastes\" :key=\"waste._id\" class=\"data-item\">\n          <td>{{ waste.name }}</td>\n          <td>{{ waste.description }}</td>\n          <td>{{ waste.quantity }}</td>\n          <td class=\"action-buttons\">\n            <button class=\"btn edit-btn\" @click=\"editWaste(waste)\">Edit</button>\n            <button class=\"btn delete-btn\" @click=\"confirmDeleteWaste(waste)\">Delete</button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n\n    <button @click=\"toggleAddWasteForm\" class=\"btn add-btn\">\n      {{ showAddWasteForm ? \"Cancel\" : \"Add Waste\" }}\n    </button>\n\n    <form v-if=\"showAddWasteForm\" @submit.prevent=\"createWaste\" class=\"form\">\n      <input v-model=\"newWaste.name\" placeholder=\"Name\" required>\n      <input v-model=\"newWaste.description\" placeholder=\"Description\" required>\n      <input v-model=\"newWaste.quantity\" type=\"number\" placeholder=\"Quantity\" required>\n      <button type=\"submit\" class=\"btn confirm-add-btn\">Add Waste</button>\n    </form>\n\n    <!-- Delete Confirmation Modal -->\n    <div v-if=\"showDeleteModal\" class=\"modal\">\n      <div class=\"modal-content\">\n        <p>Are you sure you want to delete this waste?</p>\n        <div class=\"modal-buttons\">\n          <button class=\"btn confirm-delete-btn\" @click=\"deleteWaste\">Yes, Delete</button>\n          <button class=\"btn cancel-btn\" @click=\"closeDeleteModal\">Cancel</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport api from '@/services/api';\n\nexport default {\n  data() {\n    return {\n      wastes: [],\n      newWaste: {\n        name: '',\n        description: '',\n        quantity: 0\n      },\n      showAddWasteForm: false,\n      showDeleteModal: false,\n      deleteWasteId: null,\n    };\n  },\n  created() {\n    this.fetchWastes();\n  },\n  methods: {\n    fetchWastes() {\n      api.getAll()\n        .then(response => {\n          this.wastes = response.data;\n        })\n        .catch(error => {\n          console.error('Error fetching wastes:', error);\n        });\n    },\n    createWaste() {\n      api.createWaste(this.newWaste)\n        .then(response => {\n          this.wastes.push(response.data);\n          this.newWaste = { name: '', description: '', quantity: 0 };\n          this.showAddWasteForm = false;\n        })\n        .catch(error => {\n          console.error('Error creating waste:', error);\n        });\n    },\n    confirmDeleteWaste(waste) {\n      this.showDeleteModal = true;\n      this.deleteWasteId = waste._id;\n    },\n    deleteWaste() {\n      api.delete(this.deleteWasteId)\n        .then(() => {\n          this.wastes = this.wastes.filter(waste => waste._id !== this.deleteWasteId);\n          this.closeDeleteModal();\n        })\n        .catch(error => {\n          console.error('Error deleting waste:', error);\n        });\n    },\n    closeDeleteModal() {\n      this.showDeleteModal = false;\n      this.deleteWasteId = null;\n    },\n    toggleAddWasteForm() {\n      this.showAddWasteForm = !this.showAddWasteForm;\n    },\n    editWaste(waste) {\n      // Implementation for editing waste\n    }\n  }\n};\n</script>\n"],"mappings":";AAkDA,OAAOA,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,QAAQ,EAAE;MACZ,CAAC;MACDC,gBAAgB,EAAE,KAAK;MACvBC,eAAe,EAAE,KAAK;MACtBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IA